<UserControl x:Class="Fielder"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Interfacer5"
             mc:Ignorable="d" 
             d:DesignHeight="86" d:DesignWidth="272">
    <UserControl.Resources>
        
    </UserControl.Resources>
          <StackPanel x:Name="MightySp" Orientation="Horizontal" HorizontalAlignment="Right" Height="0" Margin="0,0,-272,-86" VerticalAlignment="Bottom" Width="272" Tag="{Binding Name}">
            <Label Content="Eye Contact (FA)"/>
            <TextBox x:Name="FA_EyeTextBox" Text="{Binding Path=(StackPanel.Tag), Mode=TwoWay, NotifyOnValidationError=true, UpdateSourceTrigger=PropertyChanged, ValidatesOnExceptions=true}"/>
        </StackPanel>
</UserControl>
